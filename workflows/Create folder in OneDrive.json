    {
      "parameters": {
        "jsCode": "const item = $input.first().json;\nconst storedIds = item.storage.storedIds || {};\n\nstoredIds[item.path] = item.id;\n\nlet parentForTheNext = null;\nif (item.parent === \"/drive/root:\") {\n  parentForTheNext = item.id;\n} else {\n  parentForTheNext = storedIds[item.parent] || null;\n}\n\nreturn [{\n  json: {\n    ...item,\n    storage: { storedIds }, // carry updated storage forward\n    parentForTheNext\n  }\n}];\n"
      },
